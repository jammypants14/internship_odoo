<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
		
		<template id='model_one_pdf_report'>
			<t t-call='web.html_container'>
				<t t-foreach='docs' t-as='o'>
						<t t-call='web.external_layout'>
							<div class='page'>
								<h2 class="text-center">Model One Report</h2>	
								<h3>Sequence Number:<span t-field="o.seq" /></h3>
								<h5>Name:<span t-field="o.name" /></h5>
								<h5>Age:<span t-field="o.age" /></h5>
								<h5>Gender:<span t-field="o.gender" /></h5>
							</div>
						</t>
					</t>
				</t>
		</template>
		
	</data>
</odoo>